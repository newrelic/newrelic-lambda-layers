{
  "name": "newrelic-lambda-layers",
  "version": "11.5.0",
  "description": "New Relic Installer for serverless APM layers.",
  "main": "",
  "files": [
    "../dist",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "npm install && mkdir -p node_modules/newrelic-lambda-wrapper && cp index.js node_modules/newrelic-lambda-wrapper && mkdir -p nodejs && cp -r node_modules nodejs && zip -rq ../dist/nodejs/NewRelicLayer.zip nodejs && rm -rf ./nodejs",
    "clean": "rm ../dist/nodejs/NewRelicLayer.zip",
    "lint": "eslint ./*.js test",
    "lint:fix": "eslint --fix ./*.js test",
    "test": "npm run lint && npm run test:unit && npm run test:integration",
    "test:unit": "rm -f newrelic_agent.log && c8 -o ./coverage/unit tap --test-regex='(\\/|^test\\/unit\\/.*\\.tap\\.js)$' --timeout=180 --no-coverage --reporter classic",
    "test:integration": "npm run test:integration:cjs && npm run test:integration:esm",
    "test:integration:cjs": "NEW_RELIC_USE_ESM=false MODULE_TYPE=cjs tap --timeout=180 --no-coverage --reporter classic test/integration/cjs/*.tap.js",
    "test:integration:esm": "NEW_RELIC_USE_ESM=true MODULE_TYPE=esm tap --timeout=180 --no-coverage --reporter classic test/integration/esm/*.tap.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newrelic/newrelic-lambda-layers.git"
  },
  "author": "newrelic",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/newrelic/newrelic-lambda-layers/issues"
  },
  "homepage": "https://github.com/newrelic/newrelic-lambda-layers#readme",
  "dependencies": {
    "newrelic": "^12.8.0"
  },
  "keywords": [
    "lambda",
    "serverless",
    "sls",
    "agent",
    "analytics",
    "metrics",
    "telemetry",
    "tracing",
    "distributed tracing",
    "layers"
  ],
  "devDependencies": {
    "@newrelic/test-utilities": "^8.1.0",
    "c8": "^7.12.0",
    "eslint": "^8.23.1",
    "eslint-plugin-import": "^2.26.0",
    "node-fetch": "^2.6.11",
    "proxyquire": "^2.1.3",
    "serverless": "^3.32.2",
    "serverless-offline": "^13.3.0",
    "tap": "^16.3.0"
  }
}
